<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<fx:root alignment="CENTER_LEFT" styleClass="eventListItemContainer" stylesheets="@EventListItem.css" type="HBox" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="label" styleClass="labelFont" text="Label" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets right="10.0" />
         </HBox.margin>
      </Label>
      <Region prefHeight="0.0" prefWidth="0.0" HBox.hgrow="ALWAYS" />
      <Button mnemonicParsing="false" onAction="#cancel" styleClass="sbutton" text="%SettledDebtsListItem.cancel" />
   </children>
   <padding>
      <Insets bottom="7.0" left="10.0" right="10.0" top="7.0" />
   </padding>
   <stylesheets>
      <URL value="@/client/scenes/global.css" />
   </stylesheets>
</fx:root>
