<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<fx:root alignment="CENTER_LEFT"  type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="debtTitleLabel" prefHeight="17.0" prefWidth="150.0" text="Label" HBox.hgrow="ALWAYS">
            <HBox.margin>
                <Insets right="10.0" />
            </HBox.margin>
        </Label>
    </children>
    <padding>
        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
    </padding>
    <cursor>
        <Cursor fx:constant="HAND" />
    </cursor>
</fx:root>
